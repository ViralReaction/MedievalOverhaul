<Defs>

	<!-- Tribal Plants -->
	<ThingDef ParentName="PlantBase">
		<defName>DankPyon_Plant_Onions</defName>
		<label>onion plant</label>
		<description>A highly nutritious bulb. Onions grow well even in even the worst soils, but don't benefit from better soil.</description>
		<descriptionHyperlinks>
			<ThingDef>DankPyon_RawOnions</ThingDef>
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>0.03</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plants/FullGrown/OnionPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<!--fertilityMin></fertilityMin-->
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>9</maxMeshCount>
			<harvestedThingDef>DankPyon_RawOnions</harvestedThingDef>
			<harvestYield>13</harvestYield>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponics</li>
			</sowTags>
			<topWindExposure>0.20</topWindExposure>
			<growDays>4</growDays>
			<fertilitySensitivity>0.2</fertilitySensitivity>
			<visualSizeRange>0.2~0.4</visualSizeRange>
			<immatureGraphicPath>Things/Plants/Immature/OnionPlant</immatureGraphicPath>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
			<sowMinSkill>3</sowMinSkill>
		</plant>
	</ThingDef>

